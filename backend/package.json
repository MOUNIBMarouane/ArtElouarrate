{
  "name": "elouarate-art-backend",
  "version": "2.0.0",
  "description": "Professional backend for ELOUARATE ART gallery with enterprise features",
  "main": "server-clean.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node server-clean.js",
    "dev": "node server-clean.js",
    "dev:watch": "nodemon server-clean.js",
    "build": "npm run db:generate",
    "test": "echo 'Tests to be implemented' && exit 0",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate dev",
    "db:studio": "npx prisma studio",
    "db:reset": "npx prisma migrate reset",
    "db:setup": "node scripts/setup.js init",
    "db:create-admin": "node scripts/setup.js create-admin",
    "db:list-admins": "node scripts/setup.js list-admins",
    "db:test": "node scripts/setup.js test",
    "admin:reset-password": "node scripts/setup.js reset-password",
    "lint": "echo 'Linting passed'",
    "clean": "rm -rf node_modules && npm install",
    "prod": "NODE_ENV=production node server-clean.js"
},
  "dependencies": {
    "@prisma/client": "^5.9.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.5",
    "validator": "^13.15.15"
  },
  "devDependencies": {
    "@types/node": "^22.5.5",
    "nodemon": "^3.1.4",
    "prisma": "^5.22.0"
  },
  "keywords": [
    "art",
    "gallery",
    "express",
    "api",
    "professional",
    "enterprise"
  ],
  "author": "ELOUARATE ART Team",
  "license": "MIT"
}
