{
  "name": "elouarate-art-professional",
  "version": "2.0.0",
  "description": "Professional full-stack art gallery with enterprise-grade performance and security",
  "main": "server-pro.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "NODE_ENV=production node server-pro.js",
    "dev": "NODE_ENV=development node server-pro.js",
    "dev:single": "NODE_ENV=development node api/routes-pro.js",
    "build": "npm run db:generate && npm run build:frontend",
    "build:frontend": "cd ../Frontend && npm run build",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "lint": "eslint . --ext .js --fix",
    "lint:check": "eslint . --ext .js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate dev",
    "db:migrate:prod": "npx prisma migrate deploy",
    "db:studio": "npx prisma studio",
    "db:seed": "node prisma/seed.js",
    "db:reset": "npx prisma migrate reset",
    "cache:clear": "node -e \"import('./lib/cache-pro.js').then(c => c.default.clear())\"",
    "logs:view": "tail -f logs/combined.log",
    "logs:error": "tail -f logs/error.log",
    "logs:clear": "rm -rf logs/*.log",
    "security:audit": "npm audit && npm audit fix",
    "security:check": "node scripts/security-check.js",
    "performance:test": "node scripts/performance-test.js",
    "health:check": "curl -f http://localhost:3001/api/health || exit 1",
    "deploy:staging": "npm run build && npm run db:migrate && pm2 start ecosystem.config.js --env staging",
    "deploy:production": "npm run build && npm run db:migrate:prod && pm2 start ecosystem.config.js --env production",
    "monitor": "pm2 monit",
    "stop": "pm2 stop all",
    "restart": "pm2 restart all",
    "reload": "pm2 reload all",
    "cluster:start": "pm2 start ecosystem.config.js",
    "cluster:stop": "pm2 stop ecosystem.config.js",
    "docker:build": "docker build -t elouarate-art .",
    "docker:run": "docker run -p 3001:3001 elouarate-art",
    "docker:compose": "docker-compose up -d"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@supabase/supabase-js": "^2.49.8",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.15",
    "redis": "^4.7.0",
    "sharp": "^0.33.5",
    "winston": "^3.14.2",
    "joi": "^17.13.3",
    "express-slow-down": "^2.0.3",
    "express-brute": "^1.0.1",
    "express-brute-redis": "^0.0.1",
    "node-cron": "^3.0.3",
    "uuid": "^10.0.0",
    "moment": "^2.30.1",
    "lodash": "^4.17.21",
    "chalk": "^5.3.0",
    "figlet": "^1.7.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.5",
    "prisma": "^5.22.0",
    "vercel": "^37.4.2",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "eslint": "^9.11.1",
    "prettier": "^3.3.3",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.4",
    "pm2": "^5.4.2",
    "cross-env": "^7.0.3",
    "concurrently": "^9.0.1"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {},
    "extensionsToTreatAsEsm": [".js"],
    "globals": {
      "NODE_OPTIONS": "--experimental-vm-modules"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [
    "art",
    "gallery",
    "express",
    "prisma",
    "professional",
    "enterprise",
    "high-performance",
    "security",
    "clustering",
    "redis",
    "jwt",
    "api",
    "fullstack"
  ],
  "author": "Elouarate Art Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/elouarate-art.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/elouarate-art/issues"
  },
  "homepage": "https://github.com/your-username/elouarate-art#readme"
} 